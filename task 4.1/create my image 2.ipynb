   "source": [
    "import numpy\n",
    "import cv2\n",
    "w,h=512,512\n",
    "A=numpy.zeros((512,512,3) ,dtype=numpy.uint8)\n",
    "for i in range(h):\n",
    "    for j in range(w):\n",
    "            A[i,j]=[i%182,j%298,(i+j)%256]  #assigning colours\n",
    "cv2.imshow(\"photo2\", A)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  }
